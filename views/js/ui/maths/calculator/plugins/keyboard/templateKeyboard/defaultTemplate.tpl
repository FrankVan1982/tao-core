<div class="calculator-keyboard">
    <div class="row">
        <button class="key operator" data-command="term" data-param="SQRT">&radic;</button>
        <button class="key operator" data-command="term" data-param="LPAR">(</button>
        <button class="key operator" data-command="term" data-param="RPAR">)</button>
        <button class="key command" data-command="clear">C</button>
        <button class="key command" data-command="clearAll">AC</button>
    </div>
    <div class="row">
        <button class="key operator" data-command="term" data-param="CBRT">&#8731</button>
        <button class="key operand" data-command="term" data-param="NUM7">7</button>
        <button class="key operand" data-command="term" data-param="NUM8">8</button>
        <button class="key operand" data-command="term" data-param="NUM9">9</button>
        <button class="key operator" data-command="term" data-param="DIV">&divide;</button>
    </div>
    <div class="row">
        <button class="key operator" data-command="term" data-param="POW NUM2">x<sup>2</sup></button>
        <button class="key operand" data-command="term" data-param="NUM4">4</button>
        <button class="key operand" data-command="term" data-param="NUM5">5</button>
        <button class="key operand" data-command="term" data-param="NUM6">6</button>
        <button class="key operator" data-command="term" data-param="MUL">&times;</button>
    </div>
    <div class="row">
        <button class="key operator" data-command="term" data-param="POW NUM3">x<sup>3</sup></button>
        <button class="key operand" data-command="term" data-param="NUM1">1</button>
        <button class="key operand" data-command="term" data-param="NUM2">2</button>
        <button class="key operand" data-command="term" data-param="NUM3">3</button>
        <button class="key operator" data-command="term" data-param="SUB">&minus;</button>
    </div>
    <div class="row">
        <button class="key operator" data-command="term" data-param="POW">x<sup>y</sup></button>
        <button class="key operand" data-command="term" data-param="NUM0">0</button>
        <button class="key operand" data-command="term" data-param="DOT">.</button>
        <button class="key execute" data-command="execute">=</button>
        <button class="key operator" data-command="term" data-param="ADD">&plus;</button>
    </div>
</div>