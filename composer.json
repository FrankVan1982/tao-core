{
    "name": "oat-sa/tao-core",
    "authors": [
        {
            "name": "Open Assessment Technologies S.A.",
            "homepage": "http://www.taotesting.com"
        }
    ],
    "description": "TAO core extension ",
    "support": {
        "forum": "https://forum.taocloud.org"
    },
    "homepage": "http://www.taotesting.com",
    "license": "GPL-2.0-only",
    "keywords": [
        "tao",
        "oat",
        "computer-based-assessment"
    ],
    "type": "tao-extension",
    "extra": {
        "tao-extension-name": "tao",
        "npm-bridge": {
            "arguments": [
                "--prefix",
                "views"
            ]
        }
    },
    "require": {
        "ext-json": "*",
        "ext-mbstring": "*",
        "ezyang/htmlpurifier": "^4.9",
        "guzzlehttp/guzzle": "6.5.5",
        "guzzlehttp/psr7": "1.6.1",
        "imsglobal/lti": "^3.0",
        "justinrainbow/json-schema": "5.2.1",
        "oat-sa/jig": "~0.1",
        "oat-sa/composer-npm-bridge": "~0.4.2||dev-master",
        "oat-sa/oatbox-extension-installer": "~1.1||dev-master",
        "oat-sa/generis": "dev-feature/php-8-1-update",
        "composer/package-versions-deprecated": "^1.11",
        "paragonie/random_compat": "^2.0",
        "phpdocumentor/reflection-docblock": "2.*",
        "pimple/pimple": "^3.0",
        "psr/http-message": "~1.0",
        "slim/slim": "^3.0",
        "symfony/dotenv": "~4.3",
        "doctrine/migrations": "3.5.0",
        "doctrine/collections": "1.6.7",
        "flow/jsonpath": "~0.5",
        "jtl-software/opsgenie-client": "2.0.2",
        "league/openapi-psr7-validator": "0.9",
        "league/csv": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "oat\\tao\\controller\\": "controller",
            "oat\\tao\\model\\": "models/classes/",
            "oat\\tao\\helpers\\": "helpers",
            "oat\\tao\\test\\": "test",
            "oat\\tao\\scripts\\": "scripts",
            "oat\\tao\\install\\": "install"
        },
        "files": [
            "includes/globalHelpers.php"
        ]
    }
}
