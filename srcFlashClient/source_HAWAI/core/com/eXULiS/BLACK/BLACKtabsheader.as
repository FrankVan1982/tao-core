import com.eXULiS.BLACK.BLACKelement;import com.eXULiS.lib.*;import com.xfactorstudio.xml.xpath.*;class com.eXULiS.BLACK.BLACKtabsheader extends BLACKelement {	private var _obj:MovieClip;	private var listItems_ar;	public var tabsheader_xml;		function BLACKtabsheader(objParent,objDef:XMLNode) {		super(objParent,objDef);	}		function create(){		trace("BLACKlistBoxDatas (create): tabsheader (" + id + ") on " + _objParent + " on depth: " + _objParent._childNextDepth);		_objParent.createEmptyMovieClip(_objDef.attributes["id"],_objParent._childNextDepth);		_obj = super.create(_obj,this,1);					listItems_ar = toolbox.wrapRun(_objDef.attributes["path"], _guiSource, "Nodes", "Xml");				tabsheader_xml=listItems_ar[0];		return _obj;	}	function destroy(){		trace("BLACKlistBoxDatas (destroy) for " + id + ": " + _objDescendants.length + " children to be deleted");		for(var vCpt=0;vCpt < _objDescendants.length;vCpt++){			_objDescendants[vCpt]._exulis.destroy();		}		this.removeMovieClip();	}}