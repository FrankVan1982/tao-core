import mx.transitions.Tween;class ElementFade{	// clip to Fade	private var clip_mc:MovieClip;	// Tween begin value	private var propBegin_nb:Number;	// Tween end value	private var propEnd_nb:Number;	// Duration in frame	private var duration_nb:Number;	// Tween Object	private var zeTween_tw:Tween;			public function ElementFade(clip_mc:MovieClip)	{		trace("New ElementFade");		var zeTween_tw;		this.clip_mc=clip_mc;		duration_nb=10;	}			public function fadeIn():Void	{		trace("ElementFade fadeIn "+" clip_mc > "+clip_mc);				zeTween_tw=new Tween(clip_mc,"_alpha",null,clip_mc._alpha,100,duration_nb);		zeTween_tw.onMotionFinished=function()		{			trace("fin du Tween");		}			}				public function fadeOut():Void	{		trace("ElementFade fadeOut "+" clip_mc > "+clip_mc);				zeTween_tw=new Tween(clip_mc,"_alpha",null,clip_mc._alpha,0,duration_nb);			}		public function fade():Void	{				trace("ElementFade fade");				zeTween_tw=new Tween(clip_mc,"_alpha",null,propBegin_nb,propEnd_nb,duration_nb);			}		public function setBegin(value_nb):Void	{		propBegin_nb=value_nb;	}			public function setEnd(value_nb):Void	{		propEnd_nb=value_nb;	}		public function setDuration(value_nb):Void	{		duration_nb=value_nb;	}		}