import mx.controls.ComboBox;import com.eXULiS.lib.*;class com.eXULiS.lib.FileSystemSortView {			private var comboItem;	private var clipRef_mc:MovieClip;	private var xulWindow_mc:MovieClip;	private var depth_nbr:Number;			public function FileSystemSortView(fileSystView_mc)	{		trace("CONSTRUCTOR [FileSystemView] list");		this.xulWindow_mc = fileSystView_mc;				clipRef_mc=xulWindow_mc;		init();	}		private function init():Void	{					//	depth_nbr=668;		//createComboBox();			}		public function displayComboBox(refToListView)	{		if (!xulWindow_mc.myCombo)		{			xulWindow_mc.createClassObject(ComboBox, "myCombo", clipRef_mc.depth_nbr);			clipRef_mc.depth_nbr++;			xulWindow_mc.myCombo.move(0,0);									//trace("VINCEXO ++ "+" > "+xulWindow_mc+" > "+xulWindow_mc.width+" > "+xulWindow_mc.height);						xulWindow_mc.myCombo.setSize(xulWindow_mc._exulis.width,xulWindow_mc._exulis.height);					} 				trace("VINCEXO REPOPULATE");				xulWindow_mc.myCombo.removeAll();		addInfosToComboBox(refToListView);					}			private function addInfosToComboBox(refToListView)	{				var _infosCol_ar:Array=refToListView.fileSystListView.getColInfos();				for (var l=0;l<_infosCol_ar.length;l++){					trace("VINCEXO "+" data > "+_infosCol_ar[l][1]+" > "+_infosCol_ar[l][0]);						xulWindow_mc.myCombo.addItem({data:_infosCol_ar[l][0], label:xulWindow_mc._exulis.toolbox.wrapRun(_infosCol_ar[l][1],xulWindow_mc._exulis._guiSource,"SingleNode","String")});						}			}			private function emptyArray(array)	{		while (array.length>0)		{			array.pop();					}	}		}