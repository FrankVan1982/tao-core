import com.eXULiS.XUL.XULelement;class com.eXULiS.XUL.XULlistitem extends XULelement {	var _obj:MovieClip;	function XULlistitem(xulParent,xulDef:XMLNode) {		super(xulParent,xulDef);		trace("XULlistitem: relay given to XULelement with xulParent = " + xulParent);	}	function create(){		trace("XULlistitem: create " + _objDef.nodeName + " (" + _objDef.attributes["id"] + ") on " + _objParent + " on depth: " + _objParent._childNextDepth);		_objParent.createEmptyMovieClip(_objDef.attributes["id"],_objParent._childNextDepth);// complete the object definition with the ancestor class definition		_obj = super.create(_obj,this,1);// not relevant/*        _obj.move(left,top);        _obj.setSize(width,height);*/// attach local properties		trace("XULlistitem: special properties setting");		var menuInitObj_obj:Object = new Object();						if(_objDef.attributes["label"] != undefined){			menuInitObj_obj.label = toolbox.wrapRun(_objDef.attributes["label"], _guiSource,"SingleNode","String");		}		else{			menuInitObj_obj.label = "???";		};		if(_objDef.attributes["data"] != undefined){			menuInitObj_obj.data = toolbox.wrapRun(_objDef.attributes["data"], _guiSource,"SingleNode","String");					} else{			menuInitObj_obj.data = "???";		};		if(_objDef.attributes["type"] != undefined){			menuInitObj_obj.type = (_objDef.attributes["type"] == "checkbox") ? "check" : _objDef.attributes["type"];		};		if(_objDef.nodeName == "menuseparator"){			menuInitObj_obj.type = "separator";			menuInitObj_obj.label = "-";		};		if(_objDef.attributes["value"] != undefined){			menuInitObj_obj.instanceName = _objDef.attributes["value"];		};		if(_objDef.attributes["name"] != undefined){			menuInitObj_obj.groupName = _objDef.attributes["name"];		};		if(_objDef.attributes["checked"] != undefined){			menuInitObj_obj.selected = _objDef.attributes["checked"];		};		if(_objDef.attributes["disabled"] != undefined){			menuInitObj_obj.enabled = ((_objDef.attributes["disabled"] == "true") || (_objDef.attributes["disabled"] == true)) ? true : false;		};		if(_actions.length > 0){			menuInitObj_obj.actions = _actions;		}/* not yet implemented		if(_objDef.attributes["image"] != undefined){			menuInitObj_obj.icon = new iconPlaceHolder();			iconPlaceHolder.setSymbol(_objDef.attributes["image"]);		}*/// delegates the item creation						        _objParent._exulis.addMenuItem(menuInitObj_obj);		return _obj;	}}