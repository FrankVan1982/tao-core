                                            </radio>
                                            <radio top="260" left="50" id="ITEM1_PROP_UID_5" label="xpath:////black:content/textcontent/label[@id='text_uid_7']">
                                            </radio>
                                            <radio top="300" left="50" id="ITEM1_PROP_UID_6" label="xpath:////black:content/textcontent/label[@id='text_uid_8']">
                                            </radio>
                                        </radiogroup>
                                    </box>
                                </hbox>
                                
                                
