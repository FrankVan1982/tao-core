                                        <checkbox top="460" left="50" id="ITEM3_PROP_UID_1" label="xpath:////black:content/textcontent/label[@id='text_uid_13']">
                                        </checkbox>
                                        <checkbox top="500" left="50" id="ITEM3_PROP_UID_2" label="xpath:////black:content/textcontent/label[@id='text_uid_14']">
                                        </checkbox>
                                        <checkbox top="540" left="50" id="ITEM3_PROP_UID_3" label="xpath:////black:content/textcontent/label[@id='text_uid_15']">
                                        </checkbox>
                                        <checkbox top="580" left="50" id="ITEM3_PROP_UID_4" label="xpath:////black:content/textcontent/label[@id='text_uid_16']">
                                        </checkbox>
