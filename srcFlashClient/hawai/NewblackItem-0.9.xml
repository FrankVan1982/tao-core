<?xml version="1.0" encoding="utf-8"?>
<black:manifest xmlns:black="http://www.exulis.lu/black.rdfs#"
 xmlns:rdfs='http://www.w3.org/TR/1999/PR-rdf-schema-19990303#'
 xmlns:rdf='http://www.w3.org/1999/02/22-rdf-syntax-ns#'
 xmlns:tao='http://www.tao.lu/tao.rdfs#'
 rdf:ID="#124764012535014">
    
    
    <!-- *********************************************
    Business Definition of your item
    ********************************************* -->
    <black:business>
        
        <tao:settings>
            <rdfs:label lang="EN">
                Computer Software Knowledge Test
            </rdfs:label>
            <rdfs:comment lang="EN">
                Computer Software Knowledge Test on Development Methods
            </rdfs:comment>
            <tao:duration>
                120
            </tao:duration>
        </tao:settings>
        
        <tao:items>
            <tao:item order="1" id="ITEM_UID_1">
                <tao:questionmaterial datasource="xpath:////layout//label[@id='ITEM1_ITEM_LABEL']/@value"/>
				<!-- AND, OR, XOR, ORX, MNF, M -->
				<!-- MNF_2 "minimum not" fail 2 good answers -->
				<!-- M_2 minimum 2 good answers -->
                <tao:itemrule>
                    AND
                </tao:itemrule>
                <tao:hasguide/>
                <tao:setvalues>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM1_PROP_UID_1']/@selected" order="1"/>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM1_PROP_UID_2']/@selected" order="2"/>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM1_PROP_UID_3']/@selected" order="3"/>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM1_PROP_UID_4']/@selected" order="4"/>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM1_PROP_UID_5']/@selected" order="5"/>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM1_PROP_UID_6']/@selected" order="6"/>
                </tao:setvalues>
                <tao:expectedvalues>
                    <!-- Functions of the scoring method the expected value could be wrote in string -->
                    <tao:expectedvalue>
                        000010
                    </tao:expectedvalue>
                </tao:expectedvalues>
            </tao:item>
            
			
            <tao:item order="2" id="ITEM_UID_2">
                <tao:questionmaterial datasource="xpath:////layout//label[@id='ITEM2_ITEM_LABEL']/@value"/>
                <tao:itemrule>
                    AND.swf
                </tao:itemrule>
                <tao:hasguide/>
                <tao:setvalues>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM2_PROP_UID_1']/@selected" order="1"/>
                    <tao:setvalue datasource="xpath:////layout//radio[@id='ITEM2_PROP_UID_2']/@selected" order="2"/>
                </tao:setvalues>
                <tao:expectedvalues>
                    <!-- Functions of the scoring method the expected value could be shared in some expected value -->
                    <tao:expectedvalue order="1">
                        0
                    </tao:expectedvalue>
                    <tao:expectedvalue order="2">
                        1
                    </tao:expectedvalue>
                </tao:expectedvalues>
            </tao:item>
            
			
            <tao:item order="3" id="ITEM_UID_3">
                <tao:questionmaterial datasource="xpath:////layout//label[@id='ITEM3_ITEM_LABEL']/@value"/>
                <tao:itemrule>
                    AND.swf
                </tao:itemrule>
                <tao:hasguide/>
                <tao:setvalues>
                    <tao:setvalue datasource="xpath:////layout//checkbox[@id='ITEM3_PROP_UID_1']/@checked" order="1"/>
                    <tao:setvalue datasource="xpath:////layout//checkbox[@id='ITEM3_PROP_UID_2']/@checked" order="2"/>
                    <tao:setvalue datasource="xpath:////layout//checkbox[@id='ITEM3_PROP_UID_3']/@checked" order="3"/>
                    <tao:setvalue datasource="xpath:////layout//checkbox[@id='ITEM3_PROP_UID_4']/@checked" order="4"/>
                    <tao:setvalue datasource="xpath:////layout//checkbox[@id='ITEM3_PROP_UID_5']/@checked" order="5"/>
                    <tao:setvalue datasource="xpath:////layout//checkbox[@id='ITEM3_PROP_UID_6']/@checked" order="6"/>
                </tao:setvalues>
                <tao:expectedvalues>
                    <tao:expectedvalue>
                        000010
                    </tao:expectedvalue>
                </tao:expectedvalues>
            </tao:item>
            
        </tao:items>
        
    </black:business>
    
    
    <!-- *********************************************
